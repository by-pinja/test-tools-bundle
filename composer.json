{
    "name": "protacon/test-tools-bundle",
    "description": "Testing and static analysis tools bundle for Symfony projects",
    "type": "symfony-bundle",
    "homepage": "https://www.protacon.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Tarmo Lepp√§nen",
            "email": "tarmo.leppanen@protacon.com"
        },
        {
            "name": "Atte Tarvainen",
            "email": "atte.tarvainen@protacon.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "bamarni/composer-bin-plugin": "^1.2",
        "symfony/config": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/process": "^4.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": { "Protacon\\Bundle\\TestToolsBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-package-install": [
            "Protacon\\Bundle\\TestToolsBundle\\Util\\Package::postPackageInstall"
        ],
        "post-package-update": [
            "Protacon\\Bundle\\TestToolsBundle\\Util\\Package::postUpdateInstall"
        ]
    },
    "suggest": {},
    "minimum-stability": "stable",
    "extra": {}
}
